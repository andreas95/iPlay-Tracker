                                <Text fill="white" style="-fx-font-family: monospace; -fx-font-weight: bold;" text="Characters:" GridPane.columnIndex="0" GridPane.rowIndex="7"/>
                                <TextField fx:id="FieldCharactersRegister" prefWidth="250" promptText="type characters..." GridPane.columnIndex="1" GridPane.rowIndex="7" />
