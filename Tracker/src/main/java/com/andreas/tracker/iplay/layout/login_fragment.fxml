                                <PasswordField fx:id="FieldRegisterPasswordAgain" prefWidth="250" promptText="type your password..." GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                <Text fill="#dd4b39" text="*" GridPane.columnIndex="2" GridPane.rowIndex="3"/>

                                <!--
